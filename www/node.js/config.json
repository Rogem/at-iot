{
	"urlPrefix" 		: "/atsb",
	"urlEndpoint" 		: "/endpoint/webhook/",
	"namespace"			: "atsb",
	"defaultParser"		: "./parser.js",
	
	"logging" : {
		"enabled" 		: true,
		"events" 		: ["Warning", "Error"],
		"prefix" 		: "[ATSB] <$TIME> $TYPE: ",
		"postfix" 		: ""
	},
	
	"storage" : {
		"prefix" 		: "$DIRNAME/var/atsb/",
		"data" 			: "/data/:YYYY/:MM/:DD/:hh:mm",
		"exitSave" 		: true,
		"devices" 		: "devices.json",
		"sensors" 		: "sensors.json",
		"users" 		: "users.json",
		"encryption" : {
			"enabled" 	: false,
			"keyMethod" : "stdin",
			"key" 		: null,
			"algorithm" : "aes-256"
		},
		"compression" 	: true
	}
}
